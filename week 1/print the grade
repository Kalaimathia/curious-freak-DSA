#include <iostream>
using namespace std;
int main() {
    int a;
    cin >> a;
    
    if (a>=90){
        cout << "Grade A";
    }else if(a>=80){
        cout << "Grade B";
    }else if(a>=60){
        cout << "Grade C";
    }else if(a>=35){
        cout << "Grade D";
    }else if(a<35){
        cout << "fail";
    }

    return 0;
}
